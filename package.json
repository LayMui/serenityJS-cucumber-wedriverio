{
  "name": "serenity-js-cucumber-webdriverio-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "serenity-bdd update",
    "clean": "rimraf target",
    "test-ios": "failsafe clean test-ios:execute test:report",
    "test-web": "failsafe clean test-web:execute test:report",
    "test-form": "failsafe clean test-form:execute test:report",
    "test-ios:execute": "wdio config/wdio.ios.app.conf.ts",
    "test-web:execute": "wdio config/wdio.conf.web.ts",
    "test-form:execute": "wdio config/wdio.conf.form.ts",
    "test:report": "serenity-bdd run --features ./features",
    "lint": "eslint --ext ts --config .eslintrc.js .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serenity-js/serenity-js-cucumber-webdriverio-template.git"
  },
  "keywords": [
    "serenity-js",
    "bdd",
    "cucumber",
    "testing",
    "webdriverio"
  ],
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/serenity-js/serenity-js-cucumber-webdriverio-template/issues"
  },
  "homepage": "https://serenity-js.org",
  "engines": {
    "node": "^12 || ^14 || ^16",
    "npm": "^6 || ^7"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.1",
    "@serenity-js/assertions": "^2.31.0",
    "@serenity-js/console-reporter": "^2.31.0",
    "@serenity-js/core": "^2.31.0",
    "@serenity-js/cucumber": "^2.31.0",
    "@serenity-js/rest": "^2.31.0",
    "@serenity-js/serenity-bdd": "^2.31.0",
    "@serenity-js/webdriverio": "^2.31.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@wdio/appium-service": "^7.10.1",
    "@wdio/cli": "^7.10.1",
    "@wdio/cucumber-framework": "^7.10.1",
    "@wdio/local-runner": "^7.10.1",
    "@wdio/spec-reporter": "^7.10.1",
    "chromedriver": "^92.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "expect-webdriverio": "^3.1.1",
    "is-ci": "^3.0.0",
    "npm-failsafe": "^0.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "wdio-appium-service": "^0.2.3",
    "wdio-chromedriver-service": "^7.2.0"
  },
  "dependencies": {
    "appium": "^1.21.0"
  }
}
